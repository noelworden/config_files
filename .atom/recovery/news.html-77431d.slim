//  ======================================================================================================
//
//  News Vertical
//
//  ======================================================================================================
- body_class 'news'
- title 'Dataminr for News'
- meta_tag :description, 'Dataminr for News delivers the earliest tips for breaking news, off-the-radar content, and pre-viral stories directly into journalists\' workflow.'

main.main.bkgd--white
  //  Header
  //  ======================================================================================================
  header.header.header--primary.header--primary--hero.header--primary--overflow-hidden.bkgd--data-pattern-dark
    .content-contain.content-contain--desktop-only.see-how-header.js-see-how-header
      .header--primary-group.see-how-original.js-header-group
        .header--primary__interior
          h1.header--primary__interior__title.type--a2.type--white News
          p.header--primary__interior__intro.type--e3.type--white Journalists in more than 400 newsrooms around the world rely on Dataminr to deliver the earliest tips to breaking news and pre-viral stories into their workflow.
          btn.header--primary__interior__btn.btn.btn--secondary.btn--center.btn--mobile-full-width.js-see-how-button.gtm-see-how data-js-ga-label="News"
            span.type--d5.type--white See How

        .headline-carousel-group
          h2.headline-carousel-group__title.type--a5.type--white.type--center Recent Alerts
          .headline-carousel.js-headline-carousel.owl-carousel
            .headline__slide.bkgd--blue-dark
            .headline__slide.bkgd--blue-dark
            .headline__slide.bkgd--blue-dark
            .headline__slide.bkgd--blue-dark
            .headline__slide.bkgd--blue-dark

      // See How
      = render 'shared/vertical-see-how/vertical-see-how'

    .see-how-bg.js-header-group
      = image_tag('globe--faded-crop.png', class: 'header--primary__globe-static is-visible-desktop', alt: 'Globe')
      = image_tag('globe--half-with-glow.png', class: 'header--primary__globe-static is-hidden--mobile-desktop', alt: 'Globe')

    //  Latest From Dataminr
    //  ======================================================================================================
    = render 'shared/details/latest'

  //  Vertical Scroll Feature
  //  ======================================================================================================
  section.feature-group.js-features.bkgd--white

    // Sidebar nav (desktop)
    // ======================================================================================================
    nav.feature-sidebar.js-feature-sidebar.bkgd--blue-medium.is-visible-desktop
      btn.feature-sidebar__item.bkgd--blue-medium.js-feature-sidebar-button.is-active
        .feature-sidebar__item__btn
          i.icon.icon-vertical-plus.icon--small.type--blue-grey
          span.feature-sidebar__item__btn__copy.type--e4.type--blue-grey.type--uppercase Stay Ahead of Breaking Stories

        .feature__header.feature-sidebar__item__main
          .feature__header__interior
            h2.feature__header__interior__title.type--uppercase
              span> class="type--a2 type--white type--block" Stay Ahead
              span.type--c2.type--white of Breaking Stories
            p.feature__header__interior__p.type--f3.type--white Get Alerted When Critical Incidents Begin

      btn.feature-sidebar__item.bkgd--blue-medium.js-feature-sidebar-button
        .feature-sidebar__item__btn
          i.icon.icon-vertical-plus.icon--small.type--blue-grey
          span.feature-sidebar__item__btn__copy.type--e4.type--blue-grey.type--uppercase Discover Stories Before They Trend

        .feature__header.feature-sidebar__item__main
          .feature__header__interior
            h2.feature__header__interior__title.type--uppercase
              span> class="type--a2 type--white type--block" Discover Stories
              span.type--c2.type--white Before They Trend
            p.feature__header__interior__p.type--f3.type--white Find Fresh Ideas Earlier

      btn.feature-sidebar__item.bkgd--blue-medium.js-feature-sidebar-button
        .feature-sidebar__item__btn
          i.icon.icon-vertical-plus.icon--small.type--blue-grey
          span.feature-sidebar__item__btn__copy.type--e4.type--blue-grey.type--uppercase Separate News From Noise

        .feature__header.feature-sidebar__item__main
          .feature__header__interior
            h2.feature__header__interior__title.type--uppercase
              span> class="type--a2 type--white type--block" Separate News
              span.type--c2.type--white From Noise
            p.feature__header__interior__p.type--f3.type--white View Alerts Directly in Workflow

    // Article 1
    // ======================================================================================================
    article.feature.js-feature
      header.feature__header.bkgd--blue-medium.is-hidden-desktop
        .content-contain
          .feature__header__interior
            h2.feature__header__interior__title.type--uppercase
              span> class="type--a2 type--white type--block" Stay Ahead
              span.type--c2.type--white of Breaking Stories
            p.type--f3.type--white Get Alerted When Critical Incidents Begin

      // Article 1: Case Study
      section.feature__content.section
        .content-contain
          .feature__content__intro
            hr.hr.hr--thin.hr--no-margin-top.bkgd--grey-light
            p.type--d7.type--blue-grey--50 Case Study: #{@vertical_studies[0].name}
            h3.type--c4.type--blue-dark = @vertical_studies[0].headline
            hr.hr.hr--thin.bkgd--grey-light

          figure.feature__content__figure.feature__content__figure--16-9
            = shrine_image_set_tag(@vertical_studies[0].image, sizes: [:xs, :sm, :md, :lg, :xl], alt: '')

          .feature__content__main
            hr.hr.hr--left.hr--30.bkgd--blue-grey
            p.feature__content__p.type--e4.type--blue-grey = @vertical_studies[0].description
            - if @vertical_studies[0].case_study_url.present?
              = link_to "/#{@vertical_studies[0].case_study_url}", { target: '_blank', class: 'btn btn--mobile-full-width' } do |link|
                span.type--d5.type--white See full case study

      // Article 1: Features
      section.feature__content.section
        .content-contain
          .feature__content__intro
            hr.hr.hr--thin.hr--no-margin-top.bkgd--grey-light
            p.type--d7.type--blue-grey--50 Features
            h3.type--c4.type--blue-dark Find the Story Sooner
            hr.hr.hr--thin.bkgd--grey-light

          figure.feature__content__figure.feature__content__figure--laptop
            = local_image_tag('solutions/news/news--01-features', 'png', ['320', '580', '768', '1024', '1272'], sizes: '100vw', class: 'img--full-width')

          .feature__content__main
            h4.type--e2.type--blue-grey Get critical details from sources on the ground.
            hr.hr.hr--left.hr--30.bkgd--blue-grey
            p.feature__content__p.type--e4.type--blue-grey The ability to process all public Tweets in real time enables Dataminr to discover and deliver the earliest alerts about sports, politics, and other compelling stories. Early alerts give journalists a head start on covering the stories that matter most to their audience, from science and technology to entertainment and fashion.

      // Article 1: From Our Customers
      section.feature__content.section
        .content-contain
          .feature__content__intro
            hr.hr.hr--thin.hr--no-margin-top.bkgd--grey-light
            p.type--d7.type--blue-grey--50 From Our Customers
            h3.type--c4.type--blue-dark Drive Speed and Efficiency
            hr.hr.hr--thin.bkgd--grey-light

          .feature__content__main
            blockquote.blockquote.blockquote--big
              p.type--f2.type--blue-grey Dataminr acts as a really fast, really important tip sheet for us. It's an essential tool that helps us make sense of half a billion Tweets a day, filtering whatâ€™s very likely to be important to our newsroom.
              cite.blockquote__cite
                / = local_image_tag('client-logos/logo--radio-france', 'png', ['40', '60'], sizes: '20vw', class: 'blockquote__cite__logo blockquote__cite__logo--max-50 type--inline-block', alt: 'Radio France logo')
                p.blockquote__cite__p.type--d7.type--inline-block.type--blue-grey Mary Nahorniak, Deputy Managing Editor of Digital, USA TODAY

    // Article 2
    // ======================================================================================================
    article.feature.js-feature.bkgd--data-background
      header.feature__header.bkgd--blue-medium.is-hidden-desktop
        .content-contain
          .feature__header__interior
            h2.feature__header__interior__title.type--uppercase
              span> class="type--a2 type--white type--block" Discover Stories
              span.type--c2.type--white Before They Trend
            p.type--f3.type--white Find Fresh Ideas Earlier

      // Article 2: Case Study
      section.feature__content.section
        .content-contain
          .feature__content__intro
            hr.hr.hr--thin.hr--no-margin-top.bkgd--grey-light
            p.type--d7.type--blue-grey--50 Case Study: #{@vertical_studies[1].name}
            h3.type--c4.type--blue-dark = @vertical_studies[1].headline
            hr.hr.hr--thin.bkgd--grey-light

          figure.feature__content__figure.feature__content__figure--16-9
            = shrine_image_set_tag(@vertical_studies[1].image, sizes: [:xs, :sm, :md, :lg, :xl], alt: '')

          .feature__content__main
            hr.hr.hr--left.hr--30.bkgd--blue-grey
            p.feature__content__p.type--e4.type--blue-grey = @vertical_studies[1].description
            - if @vertical_studies[1].case_study_url.present?
              = link_to "/#{@vertical_studies[1].case_study_url}", { target: '_blank', class: 'btn btn--mobile-full-width' } do |link|
                span.type--d5.type--white See full case study

      // Article 2: Features
      section.feature__content.section
        .content-contain
          .feature__content__intro
            hr.hr.hr--thin.hr--no-margin-top.bkgd--grey-light
            p.type--d7.type--blue-grey--50 Features
            h3.type--c4.type--blue-dark Maintain Your Head Start
            hr.hr.hr--thin.bkgd--grey-light

          figure.feature__content__figure.feature__content__figure--16-9
            = local_image_tag('solutions/news/news--02-features', 'png', ['250', '320', '580', '768', '1024', '1280', '1400'], sizes: '(min-width: 768px) 50vw, 100vw', class: 'img--full-width')

          .feature__content__main
            h4.type--e2.type--blue-grey Stay in front of fast-moving events.
            hr.hr.hr--left.hr--30.bkgd--blue-grey
            p.feature__content__p.type--e4.type--blue-grey With a single click, the Track Story feature enables reporters and editors to receive emerging details as breaking news unfolds. Automatic real-time updates help newsrooms maintain an edge on pre-viral stories and alert readers as changes happen.

      // Article 2: From Our Customers
      section.feature__content.section
        .content-contain
          .feature__content__intro
            hr.hr.hr--thin.hr--no-margin-top.bkgd--grey-light
            p.type--d7.type--blue-grey--50 From Our Customers
            h3.type--c4.type--blue-dark Drive Insightful Reporting
            hr.hr.hr--thin.bkgd--grey-light

          .feature__content__main
            blockquote.blockquote.blockquote--big
              p.type--f2.type--blue-grey Dataminr has alerted us to a number of major news events ahead of other sources, including the bombing of the Brussels airport and the 14 July Nice attack. Enabling us to start our newsgathering process earlier allows us to focus on the best ways to tell the story.
              cite.blockquote__cite
                p.blockquote__cite__p.type--d7.type--inline-block.type--blue-grey Laurent Frisch, Head of Digital, Radio France

    // Article 3
    // ======================================================================================================
    article.feature.js-feature
      header.feature__header.bkgd--blue-medium.is-hidden-desktop
        .content-contain
          .feature__header__interior
            h2.feature__header__interior__title.type--uppercase
              span> class="type--a2 type--white type--block" Separate News
              span.type--c2.type--white From Noise
            p.type--f3.type--white View Alerts Directly in Workflow

      // Article 3: Case Study
      section.feature__content.section
        .content-contain
          .feature__content__intro
            hr.hr.hr--thin.hr--no-margin-top.bkgd--grey-light
            p.type--d7.type--blue-grey--50 Case Study: #{@vertical_studies[2].name}
            h3.type--c4.type--blue-dark = @vertical_studies[2].headline
            hr.hr.hr--thin.bkgd--grey-light

          figure.feature__content__figure.feature__content__figure--16-9
            = shrine_image_set_tag(@vertical_studies[2].image, sizes: [:xs, :sm, :md, :lg, :xl], alt: '')

          .feature__content__main
            hr.hr.hr--left.hr--30.bkgd--blue-grey
            p.feature__content__p.type--e4.type--blue-grey = @vertical_studies[2].description
            - if @vertical_studies[2].case_study_url.present?
              = link_to "/#{@vertical_studies[2].case_study_url}", { target: '_blank', class: 'btn btn--mobile-full-width' } do |link|
                span.type--d5.type--white See full case study

      // Article 3: Features
      section.feature__content.section
        .content-contain
          .feature__content__intro
            hr.hr.hr--thin.hr--no-margin-top.bkgd--grey-light
            p.type--d7.type--blue-grey--50 Features
            h3.type--c4.type--blue-dark Receive Alerts Seamlessly
            hr.hr.hr--thin.bkgd--grey-light

          figure.feature__content__figure.feature__content__figure--desktop
            = local_image_tag('solutions/news/news--03-features', 'png', ['320', '580', '768', '1024', '1272'], sizes: '100vw', class: 'img--full-width')

          .feature__content__main
            h4.type--e2.type--blue-grey Alerts arrive directly within trusted tools.
            hr.hr.hr--left.hr--30.bkgd--blue-grey
            p.feature__content__p.type--e4.type--blue-grey Fast-paced newsrooms depend on efficient workflow that supports timely, informed editorial decisions. Dataminr offers flexible delivery, so journalists can receive alerts seamlessly through a web-based dashboard, email, mobile app, Slack, TweetDeck or API integration.

      // Article 3: From Our Customers
      section.feature__content.section
        .content-contain
          .feature__content__intro
            hr.hr.hr--thin.hr--no-margin-top.bkgd--grey-light
            p.type--d7.type--blue-grey--50 From Our Customers
            h3.type--c4.type--blue-dark Designed for Collaboration
            hr.hr.hr--thin.bkgd--grey-light

          .feature__content__main
            blockquote.blockquote.blockquote--big
              p.type--f2.type--blue-grey Dataminrâ€™s Slack integration is indispensable for us at The Huffington Post. Weâ€™ve created channels for different news events and coverage areas, and are able to discuss alerts as they flow in. Itâ€™s a great way to collaborate in real time with HuffPostâ€™s hundreds of journalists around the world.
              cite.blockquote__cite
                p.blockquote__cite__p.type--d7.type--inline-block.type--blue-grey Ethan Klapper, Global Social Media Editor at The Huffington Post

