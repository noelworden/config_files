  section.section.section--logos.section--logos--padding-btm.bkgd--data-pattern.js-feature-next-module
    // Technology
    article.content-contain.content-contain--960-with-margins.content-contain--padding-top-30
      header.header__hr-title-group.type--center
        h3.header__hr-title.type--d5.type--blue-dark.type--inline-block Dataminr technology is trusted by

      ul.section-logos.section-logos--tight
        li.section-logos__item
          = local_image_tag('client-logos/logo--cnn', 'png', ['180', '320'], sizes: '(min-width: 1024px) 20vw, (min-width: 768px) 25vw, 33vw', class: 'section-logos__item__img', alt: 'CNN logo')
        li.section-logos__item
          = local_image_tag('client-logos/logo--the-telegraph', 'png', ['180', '320'], sizes: '(min-width: 1024px) 20vw, (min-width: 768px) 25vw, 33vw', class: 'section-logos__item__img', alt: 'The Telegraph logo')
        li.section-logos__item
          = local_image_tag('client-logos/logo--franceinfo', 'png', ['180', '320'], sizes: '(min-width: 1024px) 20vw, (min-width: 768px) 25vw, 33vw', class: 'section-logos__item__img', alt: 'Franceinfo logo')
        li.section-logos__item
          = local_image_tag('client-logos/logo--the-sun', 'png', ['180', '320'], sizes: '(min-width: 1024px) 20vw, (min-width: 768px) 25vw, 33vw', class: 'section-logos__item__img', alt: 'Aljazeera logo')
        li.section-logos__item
          = local_image_tag('client-logos/logo--mashable', 'png', ['180', '320'], sizes: '(min-width: 1024px) 20vw, (min-width: 768px) 25vw, 33vw', class: 'section-logos__item__img', alt: 'Mashable logo')
        li.section-logos__item
          = local_image_tag('client-logos/logo--now-this', 'png', ['140', '280'], sizes: '(min-width: 1024px) 20vw, (min-width: 768px) 25vw, 33vw', class: 'section-logos__item__img section-logos__item__img--now-this', alt: 'Now This logo')

    .content-contain.content-contain--960-with-margins
      hr.hr.hr--thick.hr--full-width.bkgd--blue-medium.section-logos-hr

    // Partnerships
    article.content-contain.content-contain--960-with-margins
      header.header__hr-title-group.type--center
        h3.header__hr-title.type--d5.type--blue-dark.type--inline-block Partnerships

      .section-logos-wrap
        ul.section-logos.section-logos--max-60
          li.section-logos__item
            = local_image_tag('client-logos/logo--cnn', 'png', ['180', '320'], sizes: '(min-width: 1024px) 20vw, (min-width: 768px) 25vw, 33vw', class: 'section-logos__item__img', alt: 'CNN logo')
          li.section-logos__item
            = local_image_tag('client-logos/logo--slack', 'png', ['180', '320'], sizes: '(min-width: 1024px) 20vw, (min-width: 768px) 25vw, 33vw', class: 'section-logos__item__img', alt: 'Slack logo')
          li.section-logos__item
            = local_image_tag('client-logos/logo--first-draft', 'png', ['180', '320'], sizes: '(min-width: 1024px) 20vw, (min-width: 768px) 25vw, 33vw', class: 'section-logos__item__img', alt: 'First Draft logo')

        = link_to 'http://info.dataminr.com/partnerships', { target: '_blank', class: 'btn btn--300-max-width btn--center' } do |link|
          span.type--block.type--d5.type--white Become a partner

  //  Contact Us
  //  ======================================================================================================
  // NOTE: When adding Designed For back in, remove js-feature-next-module class below
  section.section.bkgd--contact-us--news.js-feature-next-module
